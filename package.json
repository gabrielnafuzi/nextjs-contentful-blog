{
  "name": "contentful-blog",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "start": "next start",
    "lint:fix": "eslint --fix",
    "lint": "next lint",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "contentful:export": "dotenv -- cross-var contentful space export --space-id=%CONTENTFUL_SPACE_ID% --management-token=%CONTENTFUL_MANAGEMENT_TOKEN% --export-dir=src/lib/contentful/__generated --content-file=export.json",
    "contentful:import": "dotenv -- cross-var contentful space import --space-id=%CONTENTFUL_SPACE_ID% --management-token=%CONTENTFUL_MANAGEMENT_TOKEN% --content-file=src/lib/contentful/__generated/export.json",
    "contentful:generate-types": "pnpm contentful:export && cf-content-types-generator src/lib/contentful/__generated/export.json -o src/lib/contentful/__generated/types -X && eslint src/lib/contentful/__generated/types/** --fix"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^15.17.1",
    "@contentful/rich-text-types": "^16.2.1",
    "@radix-ui/react-slot": "^1.0.2",
    "@t3-oss/env-nextjs": "^0.6.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "contentful": "^10.4.2",
    "lucide-react": "^0.268.0",
    "next": "13.4.19",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "^1.14.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.4.13",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "20.5.1",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "autoprefixer": "10.4.14",
    "cf-content-types-generator": "^2.12.2",
    "contentful-cli": "^2.6.43",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^7.3.0",
    "eslint": "8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "8.4.27",
    "prettier": "^3.0.1",
    "prettier-plugin-tailwindcss": "^0.5.1",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6"
  }
}
