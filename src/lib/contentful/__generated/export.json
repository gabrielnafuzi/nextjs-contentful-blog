        "id": "5J0ZDC3TwKzV8dDOk3mjWV",
        "type": "Entry",
        "createdAt": "2023-08-21T17:21:40.149Z",
        "updatedAt": "2023-08-21T21:56:08.549Z",
        "environment": {
          "sys": {
            "id": "master",
