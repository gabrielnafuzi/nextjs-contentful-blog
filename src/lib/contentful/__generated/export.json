          }
        },
        "publishedVersion": 36,
        "publishedAt": "2023-08-21T21:56:08.549Z",
        "firstPublishedAt": "2023-08-21T17:22:25.897Z",
        "createdBy": {
          "sys": {
            "type": "Link",
